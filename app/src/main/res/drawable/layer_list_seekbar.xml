<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--设置背景色（白色）-->
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="1dp"/>
            <size android:height="3dp" android:width="5dp" />
            <gradient android:startColor="@color/seekbar_bg"
                android:endColor="@color/seekbar_bg"/>
        </shape>
    </item>
    <!--设置进度条红色-->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <size android:height="3dp" android:width="5dp"/>
                <corners android:radius="1dp"/>
                <gradient android:startColor="@color/seekbar_progress"
                    android:endColor="@color/seekbar_progress"/>
            </shape>
        </clip>
    </item>
</layer-list>